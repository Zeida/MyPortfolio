---
import BaseHead from "../components/BaseHead.astro";
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
import JobExperienceComponent from "../components/JobExperience.astro";
import { jobs } from "../consts";
---

<!doctype html>
<html lang="en">
  <head>
    <BaseHead
      title="Job Experience"
      description="Details of my job experience"
    />
  </head>
  <body>
    <Header />
    <main>
      <JobExperienceComponent jobs={jobs} />
    </main>
    <Footer />
  </body>
</html>
